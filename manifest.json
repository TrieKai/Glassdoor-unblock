{
  "name": "Glassdoor De Block",
  "version": "1.0",
  "description": "Glassdoor De Block!",
  "browser_action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.glassdoor.com/*"
      ],
      "js": [
        "/content_script.js"
      ],
      "run_at": "document_end"
    }
  ],
  "manifest_version": 2
}